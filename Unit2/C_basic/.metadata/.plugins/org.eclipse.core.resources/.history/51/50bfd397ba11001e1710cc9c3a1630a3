#include <stdio.h>
#include <stdlib.h>

int main(void) {
	float x,y;
	printf("Enter two numbers:");
	fflush(stdin); fflush(stdout);
	scanf("%d%d",&x,&y);
	printf("The product=%d",x*y);
}
