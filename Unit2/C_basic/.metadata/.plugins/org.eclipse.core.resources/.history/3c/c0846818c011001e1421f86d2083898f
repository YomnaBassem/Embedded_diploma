#include <stdio.h>
#include <stdlib.h>

int main(void) {
	float a,b;
	fflush(stdin); fflush(stdout);
	scanf("%f",&a);
	scanf("%f",&b);
	printf("Enter the value of a:%f \n Enter the value of b:%f",a,b);
	a=b;
	b=a;
	printf("\nAfter swapping , value of a: %f \n After swapping , value of b: %f",a,b);

}
