#include <stdio.h>
#include <stdlib.h>

int main(void) {
	char x;
	char y[]={'a','e','i','o','u'};
		printf("Enter an alphabet");
		fflush(stdin); fflush(stdout);
		scanf("%c",&x);
		if(x==y){
			printf("%d is a vowel",x);
		}
		else{
			printf("%d is a consonant");
		}
}
