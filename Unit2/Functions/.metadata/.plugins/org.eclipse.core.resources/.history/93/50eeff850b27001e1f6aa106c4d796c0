#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int x,y,IsPrime;
	printf("Enter two numbers(intervals):");
	fflush(stdin); fflush(stdout);
	scanf("%d %d",&x,&y);
	for(int i=2;i>x && i<y;i++){
		IsPrime=1; //Number is prime
		for(int j=2;j>x && j<y;j++){
			if (i%j==0) IsPrime=0; //Number is not prime
		}
	if(IsPrime==1) printf("%d \t",i);
}
}
